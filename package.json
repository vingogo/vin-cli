{
  "name": "@vingogo/vin-cli",
  "version": "0.3.1",
  "type": "module",
  "description": "vin-cli",
  "private": false,
  "bin": {
    "vin": "./lib/index.js"
  },
  "main": "./lib/main.js",
  "typings": "./types/index.d.ts",
  "files": [
    "lib",
    "templates",
    "types"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "dev": "tsc --watch",
    "publish": "npm publish --access public",
    "commit": "cz",
    "commitlint": "commitlint --edit $1",
    "lint-staged": "lint-staged",
    "husky": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vingogo/vin-cli.git"
  },
  "author": "Alvin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vingogo/vin-cli/issues"
  },
  "homepage": "https://github.com/vingogo/vin-cli#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc.js --write"
    ],
    "**/*.json": [
      "prettier --config ./.prettierrc.js --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/hash-sum": "^1.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^16.11.7",
    "@types/require-from-string": "^1.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2"
  },
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@dcloudio/vite-plugin-uni": "^3.0.0-alpha-3070420230224002",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.47",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.5",
    "commander": "^8.3.0",
    "core-js": "^3.21.1",
    "esbuild": "0.13.15",
    "fs-extra": "^10.0.0",
    "glob": "^9.3.0",
    "hash-sum": "^2.0.0",
    "inquirer": "^9.1.5",
    "prettier": "^2.3.0",
    "require-from-string": "^2.0.2",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "vite": "^3.1.8"
  }
}
